class HelloWorld < Superclass {
  init(name) {
    this.name = name;
  }

  sayHello() {
    print "Hello " + this.name + "!";
  }
}

fun sayHello(name) {
    print "Hello " + name + "!";
}

var hello = HelloWorld();
hello.init()
hello.name = "World";
var method = hello.sayHello;
method();


var hello2 = HelloWorld();
hello2.name = "All";
hello2.sayHello();
